# Integration test environment variables
# Copy this file to .env.test and fill in your values
# .env.test is gitignored to protect secrets

# 25-word mnemonic for a TESTNET account with at least 5 ALGO
# Get testnet ALGO from: https://bank.testnet.algorand.network/
TEST_FUNDING_MNEMONIC="word1 word2 word3 ... word25"

# Store passphrase (empty string for no passphrase)
TEST_PASSPHRASE=""

# Disable memory locking (required for non-root testing)
DISABLE_MEMORY_LOCK=1
